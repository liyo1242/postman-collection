		{
			"name": "上班打卡",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"上班打卡成功\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"var Today = pm.collectionVariables.get(\"Today\");",
							"pm.test(\"上班打卡時間是今天\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.Data.ClockTime).to.include(Today);",
							"});",
							"const jsonResponse = pm.response.json();",
							"pm.globals.set(\"ClockId\", jsonResponse.Data.ClockId);"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.sendRequest({",
							"    url: 'https://local.kingnetsmart.com.tw/mvc/api/Login/CheckAccountPassword',",
							"    method: 'POST',",
							"    header: {",
							"        'content-type': 'application/json',",
							"    },",
							"    body: {",
							"        mode: 'raw',",
							"        raw: JSON.stringify({",
							"            \"Account\": \"0921229318\",",
							"            \"Password\": \"850404\",",
							"            \"Latitude\": 0,",
							"            \"Longitude\": 0",
							"        })",
							"    }",
							"}, function (err, response) {",
							"    const jsonResponse = response.json();",
							"    pm.collectionVariables.set(\"housekeeperToken\", jsonResponse.Data.Token);",
							"    pm.collectionVariables.set(\"Com_id\", jsonResponse.Data.communities[1].Id);",
							"});",
							"const housekeeperToken = pm.collectionVariables.get(\"housekeeperToken\");",
							"const Com_id = pm.collectionVariables.get(\"Com_id\");",
							"",
							"",
							"",
							"setTimeout(function () {",
							"    pm.sendRequest({",
							"        url: 'https://local.kingnetsmart.com.tw/api/Login/CommunityLogin',",
							"        method: 'POST',",
							"        header: {",
							"            'content-type': 'application/json',",
							"        },",
							"        body: {",
							"            mode: 'raw',",
							"            raw: JSON.stringify({",
							"                \"Token\": pm.collectionVariables.get(\"housekeeperToken\"),",
							"                \"ComId\": pm.collectionVariables.get(\"Com_id\"),",
							"            })",
							"        }",
							"    }, function (err, response) {",
							"        const jsonResponse2 = response.json();",
							"    })",
							"}, 1000);",
							"",
							"Date.prototype.yyyymmdd = function() {",
							"  var mm = this.getMonth() + 1; // getMonth() is zero-based",
							"  var dd = this.getDate();",
							"",
							"  return [this.getFullYear(),",
							"          (mm>9 ? '' : '0') + mm,",
							"          (dd>9 ? '' : '0') + dd",
							"         ].join('-');",
							"};",
							"var date = new Date();",
							"pm.collectionVariables.set(\"Today\", date.yyyymmdd());"
						],
						"type": "text/javascript"
					}
				}
			],
