							"    var jsonData = pm.response.json();",
							"    console.log(jsonData.Data.ManagerClocks[0].ClockId)",
							"    pm.expect(jsonData.Data.ManagerClocks[0].ClockId).to.eql(ClockId);",
							"});",
							"",
							"",
							"const jsonData = pm.response.json();",
							"const managerClock = jsonData.Data.ManagerClocks;",
							"const length = managerClock.length",
							"const a = []",
							"const managerClockId = managerClock.forEach((item, index, arr) => a.push(arr[index].ClockId));",
							"console.log(a);",
							"console.log(a.sort((x,y) => x-y));",
							"console.log(a.sort((x,y) => y-x));"
